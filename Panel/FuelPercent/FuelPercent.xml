<?xml version="1.0"?>

<PropertyList>

<path>FuelPercent.ac</path>

<animation>
<type>material</type>
<object-name>needle1</object-name>
<object-name>needle2</object-name>
<object-name>face</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<type>rotate</type>
<object-name>needle1</object-name>
<property>consumables/fuel/total-fuel-norm</property>
<factor>1</factor>
<center>
<x>0</x>
<y>0</y>
<z>0</z>
</center>
<interpolation>
<entry><ind>0</ind><dep>0</dep></entry>
<entry><ind>1.0</ind><dep>270</dep></entry>
</interpolation>
<axis>
<x>-1</x>
<y> 0</y>
<z> 0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>needle2</object-name>
<property>consumables/fuel/total-fuel-norm</property>
<factor>1</factor>
<center>
<x-m>0</x-m>
<y-m>-0.007</y-m>
<z-m>0.0082</z-m>
</center>
<interpolation>
<entry><ind>0</ind><dep>0</dep></entry>
<entry><ind>0.05</ind><dep>180</dep></entry>
<entry><ind>0.1</ind><dep>360</dep></entry>
<entry><ind>1</ind><dep>3600</dep></entry>
</interpolation>
<axis>
<x>-1</x>
<y> 0</y>
<z> 0</z>
</axis>
</animation>

</PropertyList>