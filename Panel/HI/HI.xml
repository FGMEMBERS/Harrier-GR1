<?xml version="1.0"?>

<PropertyList>

<path>HI.ac</path>

<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>hinum100</object-name>
<object-name>hinum10</object-name>
<object-name>hinum1</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission> 
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<type>rotate</type>
<object-name>face</object-name>
<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>sphere</object-name>
<property>instrumentation/magnetic-compass/bookmark-heading-deg</property>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>hinum100</object-name>
<property>/instrumentation/heading-indicator/indicated-heading-deg</property>
<factor>0.001</factor>
<step>100</step>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>hinum10</object-name>
<property>/instrumentation/heading-indicator/indicated-heading-deg</property>
<factor>0.01</factor>
<step>10</step>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>hinum1</object-name>
<property>/instrumentation/heading-indicator/indicated-heading-deg</property>
<factor>0.1</factor>
<step>1</step>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

</PropertyList>