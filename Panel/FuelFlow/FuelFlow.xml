<?xml version="1.0"?>

<PropertyList>

<path>FuelFlow.ac</path>

<animation>
<type>material</type>
<object-name>needle</object-name>
<object-name>face</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<name>needle</name>
<type>rotate</type>
<object-name>needle</object-name>
<property>engines/engine/fuel-flow-gph</property>
<interpolation>
<entry><ind>0</ind><dep>0</dep></entry>
<entry><ind>2400</ind><dep>360</dep></entry>
</interpolation>
<center>
<x-m>0</x-m>
<y-m>0</y-m>
<z-m>0</z-m>
</center>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

</PropertyList>
