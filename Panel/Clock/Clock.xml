<?xml version="1.0"?>

<PropertyList>

<path>Clock.ac</path>

<animation>
<type>material</type>
<object-name>HourHand</object-name>
<object-name>MinuteHand</object-name>
<object-name>SecondHand</object-name>
<object-name>calculator-ring</object-name>
<object-name>face</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation> 
<type>rotate</type>
<object-name>HourHand</object-name>
<property>/instrumentation/clock/indicated-sec</property>
<factor>0.00833333333333333</factor>
<axis> 
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>
<animation> 
<type>rotate</type>
<object-name>MinuteHand</object-name>
<property>/instrumentation/clock/indicated-sec</property>
<factor>0.1</factor>
<axis> 
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>
<animation> 
<type>rotate</type>
<object-name>SecondHand</object-name>
<property>/instrumentation/clock/indicated-sec</property>
<factor>6</factor>
<axis> 
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

</PropertyList>