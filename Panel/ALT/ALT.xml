<?xml version="1.0"?>

<PropertyList>

<path>ALT.ac</path>

<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>needle100</object-name>
<object-name>ft10000</object-name>
<object-name>ft1000</object-name>
<object-name>mb1000</object-name>
<object-name>mb100</object-name>
<object-name>mb10</object-name>
<object-name>mb1</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<type>rotate</type>
<object-name>needle100</object-name>
<object-name>axis</object-name>
<property>instrumentation/altimeter/indicated-altitude-ft</property>
<factor>0.36</factor>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>ft1000</object-name>
<property>/instrumentation/altimeter/indicated-altitude-ft</property>
<factor>0.0001</factor>
<step>1000</step>
<scroll>20</scroll>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>ft10000</object-name>
<property>/instrumentation/altimeter/indicated-altitude-ft</property>
<factor>0.00001</factor>
<step>10000</step>
<scroll>20</scroll>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>mb1000</object-name>
<property>/instrumentation/altimeter/millibars</property>
<factor>0.0001</factor>
<step>1000</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>mb100</object-name>
<property>/instrumentation/altimeter/millibars</property>
<factor>0.001</factor>
<step>100</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>mb10</object-name>
<property>/instrumentation/altimeter/millibars</property>
<factor>0.01</factor>
<step>10</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>mb1</object-name>
<property>/instrumentation/altimeter/millibars</property>
<factor>0.1</factor>
<step>1</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

</PropertyList>
