<?xml version="1.0"?>

<PropertyList>

<path>AI.ac</path>

<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>ring</object-name>
<object-name>pitch</object-name>
<object-name>bezel</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission> 
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<type>rotate</type>
<object-name>face</object-name>
<object-name>PitchTranslation</object-name>
<property>orientation/roll-deg</property>
<factor>-1</factor>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>translate</type>
<object-name>pitch</object-name>
<property>orientation/pitch-deg</property>
<factor>-0.00035</factor>
<min-m>-0.01575</min-m>
<max-m>0.01575</max-m>
<axis>
<x>0.0</x>
<y>0.0</y>
<z>1.0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>ring</object-name>
<property>orientation/roll-deg</property>
<factor>-1</factor>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

</PropertyList>