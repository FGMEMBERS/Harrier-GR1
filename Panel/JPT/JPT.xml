<?xml version="1.0"?>

<PropertyList>

<path>JPT.ac</path>

<animation>
<type>material</type>
<object-name>face</object-name>
<object-name>needle</object-name>
<object-name>egt10</object-name>
<object-name>egt1</object-name>
<object-name>egt0.1</object-name>
<object-name>egt0.01</object-name>
<emission>
<red>0.6</red>
<green>0.3</green>
<blue>0.3</blue>
</emission>
</animation>

<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

<animation>
<type>rotate</type>
<object-name>needle</object-name>
<property>/engines/engine[0]/egt-degf</property>
<factor>0.16</factor>
<center>
<x-m>0</x-m>
<y-m>0</y-m>
<z-m>0</z-m>
</center>
<axis>
<x>-1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>egt10</object-name>
<property>/engines/engine[0]/egt-degf</property>
<factor>0.0001</factor>
<step>1000</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>egt1</object-name>
<property>/engines/engine[0]/egt-degf</property>
<factor>0.001</factor>
<step>100</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>egt0.1</object-name>
<property>/engines/engine[0]/egt-degf</property>
<factor>0.01</factor>
<step>10</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>textranslate</type>
<object-name>egt0.01</object-name>
<property>/engines/engine[0]/egt-degf</property>
<factor>0.1</factor>
<step>1</step>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

</PropertyList>
